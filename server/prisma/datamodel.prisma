type User {
  id: ID! @id

  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt

  githubUserId: Int! @unique

  name: String!
  bio: String!
  public_repos: Int!
  public_gists: Int!
  likes:String
  notes: [Note!]! @relation(name: "UserNote")
}

type Note {
  id: ID! @id
  owner: User! @relation(name: "UserNote")

  text: String!
}
